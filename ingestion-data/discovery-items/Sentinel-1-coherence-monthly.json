{
    "collection": "sentinel-1-asf-subdaily",
    "bucket": "nasa-disasters",
    "prefix": "drcs_activations_new/Sentinel-1/AZI/",
    "cogify": false,
    "discovery": "s3",
    "dry_run": false,
    "filename_regex": ".*.tif$",
    "assets": {
        "azi": {
            "description": "azi",
            "regex": ".*_AZI_.*\\.tif$",
            "title": "azi"
