{
    "collection": "sentinel-1-opera-dist",
    "bucket": "nasa-disasters",
    "prefix": "drcs_activations_new/Sentinel-1/opera_dist/",
    "cogify": false,
    "discovery": "s3",
    "dry_run": false,
    "filename_regex": ".*_day.tif$",
    "assets": {
        "gen-dist": {
            "description": "gen-dist",
            "regex": ".*(_GEN-DIST-STATUS_).*\\.tif$",
            "title": "gen-dist"
        }
    },
    "id_regex": ".*(\\d{4}-\\d{2}-\\d{2}_day)\\.tif$",
    "id_template": "sentinel-1-opera-dist-{}"
}