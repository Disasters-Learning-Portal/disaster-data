      },
      "S2A_MSIL2A": {
        "description": "S2A MSIL color infrared imagery composites.",
        "regex": ".*S2A_MSIL2A_colorInfrared.*.tif$",
        "title": "Sentinel-2 A Mission Standard Image Level (MSIL) Color Infrared"
      },
      "S2B": {
        "description": "S2B color infrared imagery composites.",
        "regex": ".*S2B_colorInfrared.*.tif$",
        "title": "Sentinel-2 B Color Infrared"
      },
      "S2B_MSIL2A": {
        "description": "S2B MSIL color infrared imagery composites.",
        "regex": ".*S2B_MSIL2A_colorInfrared.*.tif$",
        "title": "Sentinel-2 B Mission Standard Image Level (MSIL) Color Infrared"
      },
      "S2C": {
        "description": "S2C color infrared imagery composites.",
        "regex": ".*S2C_colorInfrared.*.tif$",
        "title": "Sentinel-2 C Mission Standard Image Level (MSIL) Color Infrared"
      }
    },
    "id_regex": "(.*)\\.tif$",
    "id_template": "{}"
