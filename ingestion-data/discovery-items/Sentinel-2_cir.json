{
    "collection": "sentinel2-colorinfrared",
    "bucket": "nasa-disasters",
    "prefix": "drcs_activations_new/Sentinel-2/cir/",
    "cogify": false,
    "discovery": "s3",
    "dry_run": false,
    "filename_regex": ".*.tif$",
    "assets": {
      "S2A": {
        "description": "S2A color infrared imagery composites.",
        "regex": ".*_S2A_colorInfrared.*.tif$",
