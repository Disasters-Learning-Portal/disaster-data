{
    "collection": "sentinel-1-sar",
    "bucket": "nasa-disasters",
    "prefix": "drcs_activations_new/Sentinel-1/SAR/",
    "cogify": false,
    "discovery": "s3",
    "dry_run": false,
    "filename_regex": ".*.tif$",
    "assets": {
        "deformation-NS": {
            "description": "deformation",
            "regex": ".*Deformation_NS.*\\.tif$",
            "title": "deformation"
        },
        "deformation-EW": {
            "description": "deformation",
            "regex": ".*Deformation_EW.*\\.tif$",
            "title": "deformation"
        }
    },
    "id_regex": ".*(\\d{4}-\\d{2}-\\d{2}_day)\\.tif$",
    "id_template": "sentinel-1-sar-{}"
}