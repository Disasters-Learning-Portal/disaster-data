{
    "collection": "sentinel-1-rgb-daily",
    "bucket": "nasa-disasters",
    "prefix": "drcs_activations_new/Sentinel-1/rgb/daily/",
    "cogify": false,
    "discovery": "s3",
    "dry_run": false,
    "filename_regex": ".*.tif$",
    "assets": {
        "n-rgb": {
            "description": "Water mask",
            "regex": ".*S1A_N_rgb_.*\\.tif$",
            "title": "Water mask"
        },
        "rgb": {
            "description": "Binary water mask",
            "regex": ".*_S1A_rgb_.*\\.tif$",
            "title": "Binary water mask"
        },
        "mosaic": {
            "description": "Binary water mask",
            "regex": ".*_S1_mosaic_.*\\.tif$",
            "title": "Binary water mask"
        }
    },
    "id_regex": ".*_(\\d{4}-\\d{2}-\\d{2}_day)\\.tif$",
    "id_template": "sentinel-1-daily-rgb-{}"
}